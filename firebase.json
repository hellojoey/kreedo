{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/questions.html",
        "destination": "/html/questions.html"
      },
      {
        "source": "/login.html",
        "destination": "/html/login.html"
      },
      {
        "source": "/register.html",
        "destination": "/html/register.html"
      },
      {
        "source": "**",
        "destination": "/html/index.html"
      }
    ]
  },
  "firestore": {
    "rules": "firestore.rules"
  }
}
